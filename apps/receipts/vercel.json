{
  "framework": "nextjs",
  "edge": false,
  "regions": ["fra1"],
  "buildCommand": "npx turbo run build --filter=@converto/receipts",
  "outputDirectory": ".next",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=120, stale-while-revalidate"
        }
      ]
    }
  ]
}

